<!DOCTYPE html>
<html lang="en">
<head>
    <title>microXchg 2016 - {{.Session.Speakers}} - {{.Session.Name}}</title>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8"/>
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">

    <!--== CSS Files ==-->
    <link href="../css/bootstrap.css" rel="stylesheet" media="screen">
    <link href="../css/style.css" rel="stylesheet" media="screen">
    <link href="../css/animate.min.css" rel="stylesheet" media="screen">
    <link href="../css/font-awesome.css" rel="stylesheet" media="screen">
    <link href="../css/owl.carousel.css" rel="stylesheet" media="screen">
    <link href="../css/flexslider.css" rel="stylesheet" media="screen">
    <link href="../css/fancySelect.css" rel="stylesheet" media="screen">
    <link href="../css/responsive.css" rel="stylesheet" media="screen">
    <link href="../css/talk.css" rel="stylesheet" media="screen">

    <!--== Google Fonts ==-->
    <link href='http://fonts.googleapis.com/css?family=Open+Sans:400,700' rel='stylesheet' type='text/css'>
    <link href='http://fonts.googleapis.com/css?family=Belgrano' rel='stylesheet' type='text/css'>

</head>
<body>
<header id="header">
    <div id="menu" class="header-menu fixed">
        <div class="container box">
            <div class="row">
                <nav role="navigation" class="col-sm-12">
                    <div class="navbar-header">
                        <button type="button" class="navbar-toggle collapsed" data-toggle="collapse"
                                data-target=".navbar-collapse">
                            <span class="sr-only">Toggle navigation</span>
                            <span class="icon-bar"></span>
                            <span class="icon-bar"></span>
                            <span class="icon-bar"></span>
                        </button>

                        <!--== Logo ==-->
								<span class="navbar-brand logo">
                                    <img src="../microxchg_logo.png" alt="microXchg logo"/>
								</span>

                    </div>
                    <div class="navbar-collapse collapse">

                        <!--== Navigation Menu ==-->
                        <ul class="nav navbar-nav">
                            <li class="current"><a href="../index.html" class="external">Home</a></li>
                        </ul>

                    </div>
                </nav>
            </div>
        </div>
    </div>

    <div class="header-cta">
    </div>

    <div class="header-bg">
        <div id="preloader">
            <div class="preloader"></div>
        </div>
        <div class="main-slider" id="main-slider">

            <!--== Main Slider ==-->
            <ul class="slides">
                <li><img src="../images/foto5.jpg" alt="Slide Image 5"/></li>
                <li><img src="../images/foto1.jpg" alt="Slide Image 1"/></li>
                <li><img src="../images/foto2.jpg" alt="Slide Image 2"/></li>
                <li><img src="../images/foto3.jpg" alt="Slide Image 3"/></li>
            </ul>

        </div>
    </div>
</header>

<div class="content">
<div class="container box">


    <!--===============================-->
    <!--== Schedule ===================-->
    <!--===============================-->
    <section id="header" class="row">
        <div class="col-sm-12">
            <div class="section-header text-left">
                <h2>{{.Session.Name}}</h2>
            </div>
        </div>
    </section>
    <section id="talk" class="row">
        <div class="col-sm-12">
        <div>
            <h3>{{.Session.Start}} - {{.Session.End}}</h3>
            <p>{{.Session.Description}}</p>
        </div>
    </section>
    {{range .Speakers}}
    <section id="bio" class="row">
        <div class="col-sm-4">
            <div class="speaker">
                <div class="speaker-info">
                    <div class="speaker-photo">
                        <img src="{{.Avatar}}" alt="{{.Name}}"/>
                    </div>
                    <!--Twitter-->
                </div>
                <h3>{{.Name}}</h3>
            </div>
        </div>
        <div class="col-sm-8">
            <p>
                {{.About}}
            </p>
        </div>
    </section>
    {{end}}
</div>


<footer id="footer">

    <div class="footer">
        <div class="container box">

            <section id="info" class="row">
                <div class="col-sm-8">

                    <!--== Social Icons ==-->
                    <ul class="social-icons">
                        <li><a href="http://twitter.com/microxchg" class="fa fa-twitter"></a></li>
                    </ul>

                </div>
                <div class="col-sm-4 text-right">
                    <div>
                        <a href="../impressum.html">Impressum</a>
                    </div>

                    <div class="copyrights">
                        All Rights Reserved. Â© 2015
                    </div>
                </div>
            </section>

        </div>
    </div>
</footer>

<!--== Javascript Files ==-->
<script src="../js/jquery-2.1.0.min.js"></script>
<script src="../js/bootstrap.min.js"></script>
<script src="../js/jquery.scrollTo.js"></script>
<script src="../js/jquery.nav.js"></script>
<script src="../js/owl.carousel.min.js"></script>
<script src="../js/jquery.flexslider.js"></script>
<script src="../js/jquery.accordion.js"></script>
<script src="../js/jquery.placeholder.js"></script>
<script src="../js/jquery.fitvids.js"></script>
<script src="../js/gmap3.js"></script>
<script src="../js/fancySelect.js"></script>
<script src="../js/main.js"></script>
<script src="https://maps.googleapis.com/maps/api/js?sensor=false"></script>



<script>
            function changePage(event) {
                if($(event.target).hasClass('external')) {
                    window.location.href = $(event.target).attr('href');
                    return;
                }
                //...
            }
            $(function () {
                $('.nav li').click( changePage );
            })


</script>
</body>
</html>
